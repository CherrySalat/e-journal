<Window x:Class="ЭлектронныйЖурналКурсовой.Вид.Окна.ОкноУченика"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ЭлектронныйЖурналКурсовой.Вид.Окна"
        mc:Ignorable="d"
        Title="ОкноУченика" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition />
        </Grid.RowDefinitions>

        
        <Frame x:Name="КадрИнформацияОПользователе" Source="../Страницы/СтраницаДляВсехИнформацияОПользователе.xaml" Grid.Column="0" Grid.RowSpan="2"/>
        <Frame x:Name="КадрЗаголовок" Source="../Страницы/СтраницаДляВсехЗаголовок.xaml" Grid.Column="1" Grid.Row="0"/>
        <Frame x:Name="КадрРабочаяПанель" Source="../Страницы/СтраницаУченикаПанельОценок.xaml"  Grid.Column="1" Grid.Row="1"/>

    </Grid>
</Window>
